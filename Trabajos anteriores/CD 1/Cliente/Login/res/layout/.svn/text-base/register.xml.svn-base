<?xml version="1.0" encoding="utf-8"?>

<TableLayout android:id="@+id/TableLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content" xmlns:android="http://schemas.android.com/apk/res/android">
	<TableRow>
		<TextView android:text="Nombre:" android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textStyle="bold" android:padding="10px" android:textSize="18sp"></TextView>
		<EditText android:text="" android:id="@+id/NombreRegister" android:layout_width="200px" android:layout_height="wrap_content" android:maxLength="20" android:maxLines="1" android:maxWidth="200px"></EditText>
	</TableRow>
	<TableRow>
		<TextView android:text="Apellidos:" android:id="@+id/TextView04" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textStyle="bold" android:padding="10px" android:textSize="18sp"></TextView>
		<EditText android:text="" android:id="@+id/ApeRegister" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLength="20" android:maxLines="1" android:maxWidth="200px"></EditText>
	</TableRow>
	<TableRow>
		<TextView android:text="Usuario:" android:id="@+id/TextView02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textStyle="bold" android:padding="10px" android:textSize="18sp"></TextView>
		<EditText android:text="" android:id="@+id/UserRegister" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLength="20" android:maxLines="1" android:maxWidth="200px"></EditText>
	</TableRow>
	<TableRow>
		<TextView android:text="Password:" android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textStyle="bold" android:padding="10px" android:textSize="18sp"></TextView>
		<EditText android:text="" android:id="@+id/PassRegister" android:layout_width="wrap_content" android:layout_height="wrap_content" android:password="true" android:maxLength="12" android:maxLines="1" android:maxWidth="200px"></EditText>
	</TableRow>
	<TableRow>
    <Button android:id="@+id/acceptRegister" android:text="Registrarse" android:layout_marginTop="10px" android:layout_width="50px"/>
    </TableRow>
    <TableRow>
	<Button android:id="@+id/cancelRegister" android:text="Cancelar" android:layout_marginTop="10px" android:layout_width="50px"/>
	</TableRow>
	
</TableLayout>



